public class TiposPrimitivos {
	public static void main(String[] args) {
		// Staff Information
		
		// Whole numeric type
		// Byte, Short, int and long;
		
		byte anosDeEmpresa = 23;
		short numeroDeVoos = 542;
		int id = 56789;
		long pontosAcumulados = 3_131_134_716L;
	}
}
