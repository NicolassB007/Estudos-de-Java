package fundamentos;

public class OperadorTernario {
	public static void main(String[] args) {
		double media = 6;
		String resultadoRecuperacao = media >= 5.0 ? "de Recuperação" : "Reprovado";
		String resultado = media >= 7.0 ? "Aprovado":"de recuperação";
		
		System.out.println("O aluno está " + resultado);
	}
}
