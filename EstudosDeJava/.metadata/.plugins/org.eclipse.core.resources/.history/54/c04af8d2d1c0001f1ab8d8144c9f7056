package fundamentos;

public class Inferencias {
	public static void main(String[] args) {
		double a = 4.5;
		System.out.println(a);
	
		// The Java itself will go give a type for variable below
		// Java will see which value the variable has and itself will give a type
		var b = 4.5; 
		System.out.println(b);
		
		var c = "Texto";
	}
}
